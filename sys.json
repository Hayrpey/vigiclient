{
 "SERVERS": ["https://www.vigibot.com"],

 "SECUREMOTEPORT": 8042,
 "VIDEOLOCALPORT": 8043,
 "AUDIOLOCALPORT": 8044,

 "LOGFILE": "/var/log/vigiclient.log",

 "WIFIFILE": "/proc/net/wireless",
 "TEMPFILE": "/sys/class/thermal/thermal_zone0/temp",
 "WIFIRATE": 250,
 "TEMPRATE": 1000,
 "CPURATE": 250,

 "CMDDIFFUSION": [
  [
   "/usr/local/vigiclient/processdiffusion",
   " /dev/video0",
   " | /bin/nc 127.0.0.1 VIDEOLOCALPORT",
   " -w 2"
  ], [
   "/usr/local/vigiclient/processdiffvideo",
   " -loglevel fatal",
   " -f fbdev",
   " -i /dev/fb0",
   " -r FPS",
   " -c:v h264_omx",
   " -profile:v baseline",
   " -b:v BITRATE",
   " -flags:v +global_header",
   " -bsf:v dump_extra",
   " -f rawvideo",
   " tcp://127.0.0.1:VIDEOLOCALPORT"
  ], [
   "/usr/local/vigiclient/opencv/colors/bin",
   " WIDTH HEIGHT FPS",
   " | /usr/bin/ffmpeg",
   " -loglevel fatal",
   " -s:v WIDTHxHEIGHT",
   " -pix_fmt bgr24",
   " -f rawvideo",
   " -i -",
   " -c:v h264_omx",
   " -profile:v baseline",
   " -b:v BITRATE",
   " -flags:v +global_header",
   " -bsf:v dump_extra",
   " -f rawvideo",
   " tcp://127.0.0.1:VIDEOLOCALPORT"
  ], [
   "/usr/local/vigiclient/opencv/imu/bin",
   " WIDTH HEIGHT FPS",
   " | /usr/bin/ffmpeg",
   " -loglevel fatal",
   " -s:v WIDTHxHEIGHT",
   " -pix_fmt bgr24",
   " -f rawvideo",
   " -i -",
   " -c:v h264_omx",
   " -profile:v baseline",
   " -b:v BITRATE",
   " -flags:v +global_header",
   " -bsf:v dump_extra",
   " -f rawvideo",
   " tcp://127.0.0.1:VIDEOLOCALPORT"
  ], [
   "/usr/local/vigiclient/opencv/lidar/bin",
   " WIDTH HEIGHT FPS",
   " | /usr/bin/ffmpeg",
   " -loglevel fatal",
   " -s:v WIDTHxHEIGHT",
   " -pix_fmt bgr24",
   " -f rawvideo",
   " -i -",
   " -c:v h264_omx",
   " -profile:v baseline",
   " -b:v BITRATE",
   " -flags:v +global_header",
   " -bsf:v dump_extra",
   " -f rawvideo",
   " tcp://127.0.0.1:VIDEOLOCALPORT"
  ], [
   "/usr/local/vigiclient/processdiffintro",
   " -loglevel fatal",
   " -stream_loop -1",
   " -re",
   " -i /usr/local/vigiclient/intro",
   " -c:v h264_omx",
   " -profile:v baseline",
   " -b:v BITRATE",
   " -flags:v +global_header",
   " -bsf:v dump_extra",
   " -f rawvideo",
   " tcp://127.0.0.1:VIDEOLOCALPORT"
  ]
 ],

 "CMDDIFFAUDIO": [
  "/usr/local/vigiclient/processdiffaudio",
  " -loglevel fatal",
  " -f alsa",
  " -ac 1",
  " -i hw:RECORDINGDEVICE,0",
  " -ar 16000",
  " -c:a pcm_s16le",
  " -f s16le",
  " tcp://127.0.0.1:AUDIOLOCALPORT"
 ],

 "CMDTTS": "/usr/bin/espeak -v fr -f /tmp/tts.txt --stdout | /usr/bin/aplay -D plughw:PLAYBACKDEVICE",

